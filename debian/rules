#!/usr/bin/make -f
# -*- makefile -*-

#export DH_VERBOSE=1

%:
        dh $@

override_dh_installchangelogs:
        dh_installchangelogs Changes.txt

override_dh_compress:
        dh_compress -X.txt -X.js

override_dh_installdocs:
        python setup.py build_sphinx
        dh_installdocs

override_dh_auto_clean:
        dh_auto_clean
        rm -rf doc/html
        rm -rf doc/doctrees
        rm -rf setup.log
